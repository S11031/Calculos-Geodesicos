        <mxCell id="40bGwvFPC04SSGKGID5u-40" value="&lt;h1 style=&quot;font-size: 16px;&quot;&gt;Mediante las siguientes ecuaciones se puede ver como se relacionan todas estas cantidades&lt;/h1&gt;" style="text;html=1;strokeColor=#A50040;fillColor=#d80073;spacing=5;spacingTop=-20;whiteSpace=wrap;overflow=hidden;rounded=0;fontColor=#ffffff;fontSize=16;" vertex="1" parent="1">
