        <mxCell id="40bGwvFPC04SSGKGID5u-11" value="&lt;h1&gt;&lt;font style=&quot;font-size: 18px;&quot;&gt;Ideas del campo magnético de la tierra&lt;/font&gt;&lt;/h1&gt;" style="text;html=1;strokeColor=#001DBC;fillColor=#0050ef;spacing=5;spacingTop=-20;whiteSpace=wrap;overflow=hidden;rounded=0;fontColor=#ffffff;" vertex="1" parent="1">
