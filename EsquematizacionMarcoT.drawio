        <mxCell id="40bGwvFPC04SSGKGID5u-31" value="&lt;h1 style=&quot;font-size: 16px;&quot;&gt;También se puede tomar en dirección Norte, Este, Nadir o sistema basado en el norte geográfico.&lt;/h1&gt;" style="text;html=1;strokeColor=#A50040;fillColor=#d80073;spacing=5;spacingTop=-20;whiteSpace=wrap;overflow=hidden;rounded=0;fontColor=#ffffff;fontSize=16;" vertex="1" parent="1">
